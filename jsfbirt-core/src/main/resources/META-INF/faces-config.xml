<?xml version="1.0" encoding="UTF-8"?>
<faces-config xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
	version="1.2">

	<component>
		<description>Actuate Wrapper</description>
		<component-type>com.exadel.jsfbirt.ActuateWrapper</component-type>
		<component-class>com.exadel.jsfbirt.component.html.ActuateWrapper</component-class>

		<component-extension>
			<component-family>com.exadel.jsfbirt.ActuateWrapper</component-family>
			<renderer-type>com.exadel.jsfbirt.ActuateRenderer</renderer-type>
		</component-extension>
	</component>
	<component>
		<description>Birt Wrapper</description>
		<component-type>com.exadel.jsfbirt.BirtWrapper</component-type>
		<component-class>com.exadel.jsfbirt.component.html.BirtWrapper</component-class>

		<component-extension>
			<component-family>com.exadel.jsfbirt.BirtWrapper</component-family>
			<renderer-type>com.exadel.jsfbirt.BirtRenderer</renderer-type>
		</component-extension>
	</component>

	<render-kit>
		<render-kit-id>HTML_BASIC</render-kit-id>
		<renderer>
			<component-family>com.exadel.jsfbirt.ActuateWrapper</component-family>
			<renderer-type>com.exadel.jsfbirt.ActuateRenderer</renderer-type>
			<renderer-class>
				com.exadel.jsfbirt.renderkit.html.ActuateWrapperRenderer</renderer-class>
		</renderer>
		<renderer>
			<component-family>com.exadel.jsfbirt.BirtWrapper</component-family>
			<renderer-type>com.exadel.jsfbirt.BirtRenderer</renderer-type>
			<renderer-class>
				com.exadel.jsfbirt.renderkit.html.BirtWrapperRenderer</renderer-class>
		</renderer>


	</render-kit>


</faces-config>
