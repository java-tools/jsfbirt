<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">
<facelet-taglib xmlns="http://java.sun.com/JSF/Facelet">
	<namespace>http://jsf4birt.components</namespace>
	<tag>
		<tag-name>actuateWrapper</tag-name>
		<component>
			<component-type>com.exadel.jsfbirt.ActuateWrapper</component-type>
			<renderer-type>com.exadel.jsfbirt.ActuateRenderer</renderer-type>
		</component>
	</tag>
	<tag>
		<tag-name>birtWrapper</tag-name>
		<component>
			<component-type>com.exadel.jsfbirt.BirtWrapper</component-type>
			<renderer-type>com.exadel.jsfbirt.BirtRenderer</renderer-type>
		</component>
	</tag>
	
	<tag>
		<tag-name>actuateUIOption</tag-name>
		<handler-class>com.exadel.jsfbirt.taglib.ActuateUIOptionHandler</handler-class>
	</tag>
	
</facelet-taglib>